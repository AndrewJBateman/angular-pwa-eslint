{
  "name": "angular-pwa-eslint",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "p:fix": "prettier --config ./.prettierrc --write \"src/{app.environments}/**/*{.ts,.html,.scss}\"",
    "p:verify": "prettier --config ./.prettierrc --list-different \"src/{app.environments}/**/*{.ts,.html,.scss}\"",
    "dev:ssr": "ng run angular-pwa-eslint:serve-ssr",
    "serve:ssr": "node docs/angular-pwa-eslint/server/main.js",
    "build:ssr": "ng build && ng run angular-pwa-eslint:server",
    "prerender": "ng run angular-pwa-eslint:prerender"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run p:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.1",
    "@angular/common": "~13.2.1",
    "@angular/compiler": "~13.2.1",
    "@angular/core": "~13.2.1",
    "@angular/forms": "~13.2.1",
    "@angular/platform-browser": "~13.2.1",
    "@angular/platform-browser-dynamic": "~13.2.1",
    "@angular/platform-server": "~13.2.1",
    "@angular/router": "~13.2.1",
    "@angular/service-worker": "~13.2.1",
    "@nguniversal/express-engine": "^13.0.2",
    "express": "^4.15.2",
    "rxjs": "~7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.1",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.15",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.1",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.5.1",
    "typescript": "~4.5.5"
  }
}
